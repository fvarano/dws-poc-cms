{
  "kind": "collectionType",
  "collectionName": "sites",
  "info": {
    "singularName": "site",
    "pluralName": "sites",
    "displayName": "Site",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "versions": {
      "versioned": true
    }
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "code": {
      "type": "string",
      "required": true,
      "regex": "master|[0-9]{7}",
      "unique": true
    },
    "market": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::market.market",
      "inversedBy": "sites"
    },
    "pages": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::page.page",
      "mappedBy": "sites"
    }
  }
}
